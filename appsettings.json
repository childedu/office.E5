{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FileLogging": {
    "DefaultPath": "log", //默认文件地址
    "DefaultMaxMB": 500, //默认最大文件大小
    "DefaultFileName": "yyyyMMdd", //默认文件名字
    "DefaultRollingType": "Day", //默认文件新建方式
    //过滤器，以.分割，逐层匹配
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "NetCoreWebApp": "Debug",
      "NewFile": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Interval": 60000, //等待一分钟(毫秒)
  "ClientId": "自己的id",
  "ClientSecret": "自己的Secret",
  "Redirect_uri": "http://localhost:53682/",
  "RedirectTokenUrl": "https://login.microsoftonline.com/common/oauth2/v2.0/token",
  "url": [
    "https://graph.microsoft.com/v1.0/me/drive/root",
    "https://graph.microsoft.com/v1.0/me/drive",
    "https://graph.microsoft.com/v1.0/drive/root",
    "https://graph.microsoft.com/v1.0/users",
    "https://graph.microsoft.com/v1.0/me/messages",
    "https://graph.microsoft.com/v1.0/me/mailFolders/inbox/messageRules",
    "https://graph.microsoft.com/v1.0/me/mailFolders/Inbox/messages/delta",
    "https://graph.microsoft.com/v1.0/me/drive/root/children",
    "https://graph.microsoft.com/v1.0/me/mailFolders",
    "https://graph.microsoft.com/v1.0/me/outlook/masterCategories",
    "https://api.powerbi.com/v1.0/myorg/apps"
  ]

}
